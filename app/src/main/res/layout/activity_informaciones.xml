<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.informaciones.InformacionesActivity">


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/id_navigation_informaciones"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:background="?android:attr/windowBackground"
        app:layout_constraintTop_toTopOf="parent"
        app:menu="@menu/navigation_informacion"/>

    <FrameLayout
        android:id="@+id/id_fragment_contenedor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/id_navigation_informaciones">

        <!--<androidx.viewpager.widget.ViewPager
            android:id="@+id/idviewpager"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </androidx.viewpager.widget.ViewPager>-->
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>